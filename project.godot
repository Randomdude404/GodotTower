config_version=4

_global_script_classes=[ {
"base": "KinematicBody2D",
"class": "obj_baddie",
"language": "GDScript",
"path": "res://Objects/Baddies/obj_baddie.gd"
}, {
"base": "KinematicBody2D",
"class": "obj_player",
"language": "GDScript",
"path": "res://Objects/Player/obj_player.gd"
} ]
_global_script_class_icons={
"obj_baddie": "",
"obj_player": ""
}

[application]

config/name="GodotTower"
config/description="Pizza Tower recreation in Godot Engine."
run/main_scene="res://Main/Game.tscn"
boot_splash/show_image=false
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"
config/quit_on_go_back=false

[autoload]

global="*res://Main/Globals.gd"
utils="*res://Main/Utils.gd"
Console="*res://addons/quentincaffeino/console/src/Console.tscn"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=960
window/size/height=540
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/quentincaffeino/console/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

